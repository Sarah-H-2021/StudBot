import json
import boto3
from studbotscrapper import parse_costs
def lambda_handler(event, context):
    s3 = boto3.resource('s3')
    print ("[INFO] Request cost data...")
    all_data = parse_costs()
    BUCKET_NAME = "hourlystudbot"
    Program = f"{all_data ['Programme']}"
    OUTPUT_NAME = f"dataKeyTest{ Program }.json"
    OUTPUT_BODY = json.dumps(all_data)
    print (f"[INFO] Saving Data to S3 {BUCKET_NAME} Bucket...")
    s3.Bucket(BUCKET_NAME).put_object(Key=OUTPUT_NAME, Body=OUTPUT_BODY)
    print (f"[INFO] Job done at { Program }")
